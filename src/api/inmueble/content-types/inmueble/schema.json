{
  "kind": "collectionType",
  "collectionName": "inmuebles",
  "info": {
    "singularName": "inmueble",
    "pluralName": "inmuebles",
    "displayName": "Inmueble",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "inmuebleName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "inmuebleName"
    },
    "Direccion": {
      "type": "text"
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "terreno": {
      "type": "float"
    },
    "construccion": {
      "type": "float"
    },
    "dormitorios": {
      "type": "integer"
    },
    "banos": {
      "type": "integer"
    },
    "estacionamientos": {
      "type": "integer"
    },
    "descripcion": {
      "type": "blocks"
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "Casa Residencial",
        "Departamento",
        "Monoambiente",
        "Casa en Condominio",
        "Quinta",
        "Local Comercial",
        "Oficina"
      ]
    },
    "precio": {
      "type": "decimal"
    },
    "ubicacion": {
      "type": "customField",
      "customField": "plugin::map-field.map"
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "isFeatured": {
      "type": "boolean",
      "default": false
    },
    "categoria": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::categoria.categoria"
    },
    "agente": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::agente.agente"
    }
  }
}
